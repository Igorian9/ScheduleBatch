    <validationRules>
        <fullName>Rule</fullName>
        <active>true</active>
        <description>Validates that the value of a custom date field is a weekday (not Saturday or Sunday).</description>
        <errorConditionFormula>CASE(MOD( Date_Published__c-DATE(1900, 1, 7), 7),
0,0,
6,0,
1)=0</errorConditionFormula>
        <errorMessage>It&apos;s Saturday or Sunday</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Rule_2</fullName>
        <active>true</active>
        <description>Ensures that only the record owner can make changes to a custom field called Personal Goal</description>
        <errorConditionFormula>AND(
 ISCHANGED(Date_Published__c),
 OwnerId&lt;&gt;$User.Id
)</errorConditionFormula>
        <errorMessage>Sorry, but you not owner</errorMessage>
    </validationRules>
